global:
    scrape_interval: 15s
    evaluation_interval: 30s
scrape_configs:
{% for label, ip in nodes_data %}
    - job_name: "{{ label }}"
      scrape_timeout: 10s
      scheme: http
      static_configs:
        - targets: [ "{{ ip }}:4000" ]
          labels:
            cl_client: "lighthouse"
            ip_address: "{{ ip }}"
{% endfor %}